@{
    ViewBag.Title = "Klienci";
}
<p>Lista Wniosków o potwierdzenie rejestracji</p>
<table>
    <head>
        <title>Rejestracja</title>
        <style type="text/css">
            table {
                border-collapse: collapse;
                width 1000px;
            }

            td {
                border: 2px solid #000000;
                padding: 5px;
            }
        </style>
    </head>
    <tbody>
        <tr class="header">
            <td>Imię</td>
            <td>Nazwisko</td>
            <td>Pesel</td>
            <td>Nick</td>
            <td>Potwierdź rejestracje</td>
        </tr>

        @for (var j = 0; j < 4; j++)
        {
            <tr>
                <td>aaaa</td>
                <td>aaaa</td>
                <td>aaaa</td>
                <td>aaaa</td>
                <td><button type="button">Potwierdź</button></td>
            </tr>
        }

    </tbody>
</table>

